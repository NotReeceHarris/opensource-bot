{
  "scripts": {
    "deploy": "node dist/deploy-commands.js",
    "start": "node dist/index.js",
    "watch": "nodemon -e js --watch dist --exec \"node ./dist/index.js\"",
    "build": "babel src --out-dir dist --extensions \".ts,.tsx\" --no-comments",
    "clear": "rimraf dist",
    "dev": "npm run clear && npm run build && concurrently \"npm run watch\" \"npm run build -- -w\""
  },
  "devDependencies": {
    "@babel/cli": "^7.24.8",
    "@babel/core": "^7.24.8",
    "@babel/preset-env": "^7.24.8",
    "@babel/preset-typescript": "^7.24.7",
    "@types/node": "^20.14.10",
    "@types/sequelize": "^4.28.20",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-preset-minify": "^0.5.2",
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.4",
    "rimraf": "^6.0.1",
    "typescript": "^5.5.3"
  },
  "dependencies": {
    "discord.js": "^14.15.3",
    "dotenv": "^16.4.5"
  }
}
